      <JFXButton fx:id="btnReject" buttonType="RAISED" layoutX="143.0" layoutY="189.0" onAction="#rejectSubmission" prefHeight="32.0" prefWidth="104.0" styleClass="cancelBtn" stylesheets="@application.css" text="Reject" />
      <JFXButton fx:id="btnAccept" buttonType="RAISED" layoutX="29.0" layoutY="189.0" onAction="#acceptSubmission" prefHeight="32.0" prefWidth="104.0" styleClass="createBtn" stylesheets="@application.css" text="Accept" />
