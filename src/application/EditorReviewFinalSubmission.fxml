      <JFXButton fx:id="btnGetResearchers" buttonType="RAISED" layoutX="149.0" layoutY="116.0" onAction="#getResearchers" style="-fx-font-size: 10;" styleClass="createBtn" stylesheets="@application.css" text="Get Researchers" visible="false" />
