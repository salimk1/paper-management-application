<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="256.0" prefWidth="387.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.EditorReviewFinalSubmissionController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="12.0" prefHeight="29.0" prefWidth="387.0" style="-fx-font-size: 18; -fx-font-weight: bold;" text="Review Final Submission" textAlignment="CENTER" />
      <JFXComboBox fx:id="selectResearcher" labelFloat="true" layoutX="117.0" layoutY="101.0" onAction="#researcherWasSelected" promptText="Select  Researcher" />
